{% extends "::base.html.twig" %}

{% block body %}
	<section class="tipousuarios">
		<h2><i class="glyphicon glyphicon-file"></i>Tipos de Usuarios</h2>
		{% if is_granted("ROLE_ADMIN") %}
		{% endif %}
			<div class="row">
			    
			</div>
		
    	<hr/>
		{% for item in list %}
			
			<div class="row">
				{% if is_granted("ROLE_ADMIN") %}
				{% endif %}  
					<div class="row">
					    <div class="col-xs-11 text-right">
							
					    </div>
				    </div>
			      
			    <div class="col-md-10 col-sm-10">
			      
			      <div class="row">
			        <div class="col-md-2">{{item.getNombre()}}</div>
			      </div>
			    </div>
			</div>
			<hr/>
		{% endfor %}

	</section>	
{% endblock %}